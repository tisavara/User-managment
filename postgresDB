psql postgres
CREATE ROLE root WITH LOGIN PASSWORD '1234'
psql postgres -U root
CREATE DATABASE usermanage;
GRANT ALL PRIVILEGES ON DATABASE usermanage TO root;
\connect usermanage
CREATE TABLE USERMANAGE(ID INT PRIMARY KEY NOT NULL, NAME TEXT NOT NULL, EMAIL text, DEPARTMENT text, JOB_TITLE text, ROLE TEXT NOT NULL, PASSWORD text);
INSERT INTO USERMANAGE (ID,NAME,EMAIL,DEPARTMENT,JOB_TITLE,ROLE,PASSWORD) VALUES (1,'Tisavara','TTTT@TT.com','IT','Manager','Admin','Aa111111');